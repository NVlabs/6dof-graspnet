                          @QT_SRC_DIR@/tools/designer/src/lib/sdk \
                          @QT_SRC_DIR@/tools/designer/src/lib/uilib \
                          @QT_SRC_DIR@/tools/qtestlib/src \
                          @QT_SRC_DIR@/tools/qdbus/src \
                          @pyside_SOURCE_DIR@
