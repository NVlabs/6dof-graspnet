                          @QT_SRC_DIR@/extensions/activeqt \
                          @QT_SRC_DIR@/tools/assistant/lib \
                          @QT_SRC_DIR@/tools/assistant/compat/lib \
                          @QT_SRC_DIR@/tools/designer/src/uitools \
